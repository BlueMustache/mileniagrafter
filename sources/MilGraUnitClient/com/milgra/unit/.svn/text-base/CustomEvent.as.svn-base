package com.milgra.unit
{

	import flash.events.Event;
	
	/*
		Tasks of Customevent
			- server as a basis for various events
			- store event identifier constants
				
	*/

	public dynamic class CustomEvent extends Event
	{
		
		public static var ACTIVATE : String = "milgra_activate";
		public static var DEACTIVATE : String = "milgra_deactivate";
		
		public static var CHANGE : String = "milgra_change";
		public static var SELECT : String = "milgra_select";
		public static var COMPLETE : String = "milgra_complete";
		public static var PROGRESS : String = "milgra_progress";
		
		public static var SUCCESS : String = "NetConnection.Connect.Success";
		public static var FAILURE : String = "NetConnection.Connect.Failed"; 
		public static var CLOSURE : String = "NetConnection.Connect.Closed";
		public static var REJECTION : String = "NetConnection.Connect.Rejected";
		
		/**
		 * CustomEvent constructor
		 * @param idX identifier
		 **/

		public function CustomEvent ( idX : String )
		{

			super( idX );

		}

	}

}